@model List<FrontIndexPanels>
@{
    var Term = (List<UITermLanguageCustomizationList>)ViewBag.UITerms;
    //ViewData["Title"] = @Term.Find(x => x.InternalName.Equals("Classifications")).TermString;

}

<h1><img src="~/images/icons/Classifications.png" width="50" height="50" />Page / dashboard</h1>
<div class=" control-section">
    <!--  DashboardLayout element declaration -->
    <ejs-dashboardlayout id="defaultLayout" columns="10">
        <e-dashboardlayout-panels>
            @foreach (var x in Model)
            {
                @if (x.PageSectionTypeId == 4)
                {
                    <e-dashboardlayout-panel id="@x.PanelId" sizeX="@x.SizeX" sizeY="@x.SizeY" row="@x.DashboardRow" col="@x.DashboardColumn">
                        <e-content-template>
                            <ejs-schedule id="@x.SubPanelId" height="250px" width="100px" selectedDate="new DateTime(2020, 8, 10)">
                                <e-schedule-eventsettings dataSource="@x.PersonalCalendars"></e-schedule-eventsettings>
                            </ejs-schedule>
                        </e-content-template>
                    </e-dashboardlayout-panel>
                }
                @if (x.PageSectionTypeId == 5)
                {
                    <e-dashboardlayout-panel id="@x.PanelId" sizeX="@x.SizeX" sizeY="@x.SizeY" row="@x.DashboardRow" col="@x.DashboardColumn">
                        <e-content-template>
                            <ejs-schedule id="@x.SubPanelId" height="250px" width="100px" selectedDate="new DateTime(2020, 8, 10)">
                                <e-schedule-eventsettings dataSource="@x.OrganizationCalendars"></e-schedule-eventsettings>
                            </ejs-schedule>
                        </e-content-template>
                    </e-dashboardlayout-panel>
                }
                @if (x.PageSectionTypeId == 6)
                {
                    <e-dashboardlayout-panel id="@x.PanelId" sizeX="@x.SizeX" sizeY="@x.SizeY" row="@x.DashboardRow" col="@x.DashboardColumn">
                        <e-content-template>
                            <ejs-schedule id="@x.SubPanelId" height="250px" width="100px" selectedDate="new DateTime(2020, 8, 10)">
                                <e-schedule-eventsettings dataSource="@x.ProjectCalendars"></e-schedule-eventsettings>
                            </ejs-schedule>
                        </e-content-template>
                    </e-dashboardlayout-panel>
                } @if (x.PageSectionTypeId == 7)
                {
                    <e-dashboardlayout-panel id="@x.PanelId" sizeX="@x.SizeX" sizeY="@x.SizeY" row="@x.DashboardRow" col="@x.DashboardColumn">
                        <e-content-template>
                            <ejs-schedule id="@x.SubPanelId" height="250px" width="100px" selectedDate="new DateTime(2020, 8, 10)">
                                <e-schedule-eventsettings dataSource="@x.EventCalendars"></e-schedule-eventsettings>
                            </ejs-schedule>
                        </e-content-template>
                    </e-dashboardlayout-panel>
                }
            }
        </e-dashboardlayout-panels>
        </ejs-dashboardlayout>
    <!-- end of dashboardlayout element -->
</div>

<style>
    /* DashboardLayout element styles  */
    #defaultLayout {
        padding: 10px;
    }

        #defaultLayout .e-panel .e-panel-container {
            vertical-align: middle;
            font-weight: 600;
            font-size: 20px;
            text-align: center;
            line-height: 160px;
        }
</style>