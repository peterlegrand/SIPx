
@model List<SIPx.API.ViewModels.MetaListSortBy>
@{
}
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead>
            <tr><th>Sort by ID</th><th>Name</th></tr>
        </thead>
        @foreach (var x in Model)

        {
            <tr>
                <td><div>@x.SortById</div></td>
                <td><div>@x.Name</div></td>
            </tr>

        }
    </table>
</div>
